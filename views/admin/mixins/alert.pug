mixin alert-success(time)
    -if (messages.success)
        div(
            class="alert alert-success"
            show-alert
            data-time=time
        ) #{messages.success} 

mixin alert-error(time)
    -if (messages.error)
        div(
            class="alert alert-danger"
            show-alert
            data-time=time
        ) #{messages.error} 